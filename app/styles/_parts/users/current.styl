.users-current

  .dashboard-details
    .meta
      display table
      overflow hidden
      text-overflow ellipsis
      width auto
      padding 0
      margin-top 2em
      li
        display table-row
        list-style none
        label,span
          display table-cell
          font-weight 400
        span
          padding-left 20px
        label
          color #999
          text-transform uppercase
          font-size 12px
