<div class="row">
  <div class="col-lg-12">
    <h2>Search: {{query}}</h2>
  </div>
</div>
<div class="row">
  <div clas="col-lg-12">
    <h4>Plugins</h2>
  </div>
</div>
{{#each plugins}}
  <div class="row">
    <div class="col-lg-12">
      <b>{{name}}</b>
    </div>
  </div>
{{/each}}
<div class="row">
  <div clas="col-lg-12">
    <h4>Users</h2>
  </div>
</div>
{{#each users}}
  <div class="row">
    <div class="col-lg-12">
      <b>{{name}}</b>
    </div>
  </div>
{{/each}}
